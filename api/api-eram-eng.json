    "our_treatments": {
      "treatment1": {
        "link": "smile_design",
        "section_title": "Eram Smile Design",
        "section_carousel": {
          "image_1": "https://www.eram-dh.com/wp-content/uploads/2019/09/image3.jpeg",
          "image_2": "https://www.eram-dh.com/wp-content/uploads/2019/09/image2.jpeg",
