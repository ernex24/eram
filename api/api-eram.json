			},
			"treatment_6": {
			  "title": "Anesthesia Treatments",
			  "description": "<p><span style=\"vertical-align: inherit;\"><span style=\"vertical-align: inherit;\"><span style=\"vertical-align: inherit;\"><span style=\"vertical-align: inherit;\"><span style=\"vertical-align: inherit;\"><span style=\"vertical-align: inherit;\"><span style=\"vertical-align: inherit;\"><span style=\"vertical-align: inherit;\">Lorem ipsum pain sit amet, consetetur sadipsing elitr, sed diam nonumy eirmod temporal invid ut ut labore et dolore magna aliquyam erat.</span></span></span></span></span></span></span></span></p>\n",
			  "image_1": "https://www.eram-dh.com/wp-content/uploads/2019/09/kids9.jpeg",
